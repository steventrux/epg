                        window.location.href = linkToRedirect;
                   }, 15000);
                }
            }
        });


		var flip = $('.flip-container');
        var played = false;

		parent.onmessage = function(e) {
			if (e.data.event === 'end'){
				console.log('FINE');
                flip.removeClass('clicked');
			}
		};

		if (flip.length > 0){


			flip.on('click',function () {
				$(this).toggleClass('clicked');
				play();
				played = true;
			});

			setTimeout(function(){
				console.log('timeout flip');
                flip.addClass('clicked');
				if (!played){
                    play();
				}
			}, 5000);
		}

