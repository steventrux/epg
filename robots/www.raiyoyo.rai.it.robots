            function getMobileOperatingSystem() {
                var userAgent = navigator.userAgent || navigator.vendor || window.opera;
                var linkToRedirect = null;
                if (/android/i.test(userAgent)) {
                    //dispositivo "Android";
                    linkToRedirect = $('#app-android').find('a').attr('href');
                    $("#app-ios").hide();

                    $('.imgLogo').on('click', function(){
                        console.log('exit');
                        window.location.href = linkToRedirect;
                    });

                    setTimeout(function(){
                        console.log('timeout');
                        window.location.href = linkToRedirect;
                   }, 15000);


                }
                if (/iPad|iPhone|iPod/.test(userAgent) && !window.MSStream) {
                    //dispositivo "iOS";
                    linkToRedirect = $('#app-ios').find('a').attr('href');
                    $("#app-android").hide();

                    $('.imgLogo').on('click', function(){
                        console.log('exit');
                        window.location.href = linkToRedirect;
